    "import numpy as np\n",
    "\n",
    "n = 4\n",
    "\n",
    "predicted = np.array([80, 90, 70, 100])\n",
    "actual = np.array([81, 95, 68, 103])\n",
    "\n",
    "relative_errors = []\n",
    "\n",
    "for i in range(n):\n",
    "    relative_errors.append(abs((predicted[i] - actual[i]) / actual[i]))\n",
    "\n",
    "max_rel_error = max(relative_errors)\n",
    "\n",
    "max_rel_error"
   ]
