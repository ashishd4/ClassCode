   "source": [
    "import numpy as np\n",
    "\n",
    "# Floating Point Summation\n",
    "\n",
    "data = np.array([1, 2e6, 4, 7])\n",
    "\n",
    "data_sum = 0.0\n",
    "error = 0.0\n",
    "\n",
    "for i in data:\n",
    "    y = i - error\n",
    "    temp = data_sum + y\n",
    "    \n",
    "    error = (temp - data_sum) - y\n",
    "    data_sum = temp"
   ]
